{
  "name": "ebudget",
  "description": "eBudget",
  "version": "0.0.87",
  "author": "BitWorks",
  "homepage": "https://ebudgetapp.com",
  "private": true,
  "scripts": {
    "clean": "ionic-app-scripts clean",
    "version-patch": "npm version patch --no-git-tag-version",
    "version": "node scripts\\sync-cordova-package-version.js",
    "inject-build-info": "node scripts\\inject-build-info.js",
    "build": "ionic-app-scripts build",
    "ionic:build": "ionic-app-scripts build --dev",
    "ionic:serve": "ionic-app-scripts serve",
    "build-production": "ionic-app-scripts build --prod --generateSourceMap false && npm run version-patch && npm run inject-build-info",
    "favicon-generate": "node node_modules\\cli-real-favicon\\real-favicon.js generate faviconDescription.json faviconData.json src\\assets\\icon && move /Y src\\assets\\icon\\favicon.ico src\\favicon.ico",
    "favicon-inject": "node node_modules\\cli-real-favicon\\real-favicon.js inject faviconData.json src src\\index_pwa.html",
    "favicon": "node node_modules\\cli-real-favicon\\real-favicon.js check-for-update --fail-on-update faviconData.json || npm run favicon-generate && npm run favicon-inject",
    "email-android-debug-apk": "node scripts\\email-android-debug-apk.js",
    "ionic-login": "ionic login ryan@bitworks.com.au Ly6gwrMZit4aoZAM08kx",
    "ionic-package-ios-adhoc-release": "ionic package build ios --prod --release --noresources --profile ebudget_adhoc_release",
    "ionic-package-download-ios-release": "node scripts\\check-wait-ionic-package.js && ionic package download -d tmp",
    "ionic-build-ios-adhoc-release": "set IONIC_ENV=prod && npm run ionic-login && npm run ionic-package-ios-adhoc-release && npm run ionic-package-download-ios-release",
    "appsendr-ios-release": "node scripts\\appsendr-ios-release.js",
    "email-ios-adhoc-release": "node scripts\\email-ios-adhoc-release.js",
    "deploy-pwa-release": "scripts\\package-pwa.bat",
    "build-deploy-all-adhoc-release": "npm run build-production && cordova build android && npm run email-android-debug-apk && npm run ionic-build-ios-adhoc-release && npm run appsendr-ios-release && npm run email-ios-adhoc-release && npm run deploy-pwa-release"
  },
