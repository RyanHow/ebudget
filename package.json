{
    "name": "ebudget",
    "description": "eBudget",
    "version": "0.0.117",
    "author": "BitWorks",
    "homepage": "https://ebudgetapp.com",
    "private": true,
    "scripts": {
        "clean": "ionic-app-scripts clean",
        "version-patch": "npm version patch --no-git-tag-version",
        "version": "node scripts/sync-cordova-package-version.js",
        "inject-build-info": "node scripts/inject-build-info.js",
        "ionic:build:after": "npm run inject-build-info",
        "favicon-generate": "node node_modules/cli-real-favicon/real-favicon.js generate faviconDescription.json faviconData.json src\\assets\\icon && move /Y src\\assets\\icon\\favicon.ico src\\favicon.ico",
        "favicon-inject": "node node_modules/cli-real-favicon/real-favicon.js inject faviconData.json src src\\index_pwa.html",
        "favicon": "node node_modules/cli-real-favicon/real-favicon.js check-for-update --fail-on-update faviconData.json || npm run favicon-generate && npm run favicon-inject",
        "build": "ionic-app-scripts build && npm run inject-build-info",
        "email-android-debug-apk": "node scripts/email-android-debug-apk.js",
        "ionic-login": "ionic login ryan@bitworks.com.au Ly6gwrMZit4aoZAM08kx",
        "ionic-package-ios-adhoc-release": "ionic package build ios --prod --release --profile ebudget_adhoc_release",
        "ionic-package-download-ios-release": "node scripts/check-wait-ionic-package.js && ionic package download -d tmp",
        "ionic-build-ios-adhoc-release": "npm run ionic-login && npm run ionic-package-ios-adhoc-release && npm run ionic-package-download-ios-release",
        "appsendr-ios-release": "node scripts/appsendr-ios-release.js",
        "email-ios-adhoc-release": "node scripts/email-ios-adhoc-release.js",
        "build-deploy-pwa-release": "npm run build --prod && scripts/package-pwa.bat",
        "build-deploy-ios-release": "npm run ionic-build-ios-adhoc-release && npm run appsendr-ios-release && npm run email-ios-adhoc-release",
        "build-deploy-android-release": "ionic cordova build android --prod --debug --device && npm run email-android-debug-apk",
        "release": "npm run version-patch && npm run build-deploy-android-release && npm run build-deploy-pwa-release && npm run build-deploy-ios-release"
  },
  "dependencies": {
    "@angular/common": "4.1.3",
    "@angular/compiler": "4.1.3",
    "@angular/compiler-cli": "4.1.3",
    "@angular/core": "4.1.3",
    "@angular/forms": "4.1.3",
    "@angular/http": "4.1.3",
    "@angular/platform-browser": "4.1.3",
    "@angular/platform-browser-dynamic": "4.1.3",
    "@ionic-native/clipboard": "4.0.0",
    "@ionic-native/core": "4.0.0",
    "@ionic-native/device": "4.0.0",
    "@ionic-native/in-app-browser": "4.0.0",
    "@ionic-native/secure-storage": "4.0.0",
    "@ionic-native/splash-screen": "4.0.0",
    "@ionic-native/sqlite": "4.0.0",
    "@ionic-native/status-bar": "4.0.0",
    "@types/big.js": "0.0.31",
    "@types/clipboard": "^1.5.33",
    "@types/json-stable-stringify": "^1.0.31",
    "@types/later": "^1.2.3",
    "@types/lokijs": "^1.2.30",
    "big.js": "^3.1.3",
    "clipboard": "^1.6.1",
    "cordova-android": "^6.2.3",
    "cordova-ios": "^4.4.0",
    "cordova-plugin-clipboard": "^1.0.0",
    "cordova-plugin-console": "^1.0.7",
    "cordova-plugin-device": "^1.1.6",
    "cordova-plugin-inappbrowser": "^1.7.1",
    "cordova-plugin-ionic-webview": "^1.1.9",
    "cordova-plugin-ios-non-exempt-encryption": "^1.0.0",
    "cordova-plugin-secure-storage": "^2.6.5",
    "cordova-plugin-splashscreen": "^4.0.3",
    "cordova-plugin-statusbar": "^2.2.3",
    "cordova-plugin-whitelist": "^1.3.2",
    "cordova-sqlite-storage": "^2.0.4",
    "cronstrue": "^0.9.3",
    "intl": "^1.2.5",
    "ionic-angular": "3.5.3",
    "ionic-plugin-keyboard": "^2.2.1",
    "ionicons": "3.0.0",
    "json-stable-stringify": "^1.0.1",
    "later": "^1.2.0",
    "lokijs": "^1.4.3",
    "moment": "^2.18.1",
    "rxjs": "5.4.0",
    "sw-toolbox": "3.6.0",
    "zone.js": "0.8.12"
  },
  "devDependencies": {
    "@ionic/app-scripts": "^2.0.2",
    "cli-real-favicon": "0.0.6",
    "google-closure-compiler": "^20170124.0.0",
    "ionic": "3.13.0",
    "keycdn": "^0.2.0",
    "mailgun-js": "^0.8.1",
    "replace-in-file": "^2.4.0",
    "typescript": "2.3.4",
    "xmlpoke": "^0.1.12"
  },
  "config": {
    "ionic_copy": "./copy.config.js"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-clipboard": {},
      "cordova-plugin-console": {},
      "cordova-plugin-device": {},
      "cordova-plugin-inappbrowser": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-whitelist": {},
      "cordova-sqlite-storage": {},
      "ionic-plugin-keyboard": {},
      "cordova-plugin-secure-storage": {},
      "cordova-plugin-ionic-webview": {},
      "cordova-plugin-ios-non-exempt-encryption": {}
    },
    "platforms": [
      "android",
      "ios"
    ]
  }
}
