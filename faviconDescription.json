
{
	"masterPicture": "resources/icon.svg",
	"iconsPath": "/",
	"design": {
		"ios": {
			"pictureAspect": "noChange",
			"assets": {
				"ios6AndPriorIcons": false,
				"ios7AndLaterIcons": true,
				"precomposedIcons": false,
				"declareOnlyDefaultIcon": true
			}
		},
		"desktopBrowser": {},
		"windows": {
			"pictureAspect": "noChange",
			"backgroundColor": "#796969",
			"onConflict": "override",
			"assets": {
				"windows80Ie10Tile": false,
				"windows10Ie11EdgeTiles": {
					"small": false,
					"medium": true,
					"big": false,
					"rectangle": false
				}
			}
		},
		"androidChrome": {
			"pictureAspect": "noChange",
			"themeColor": "#796969",
			"manifest": {
				"display": "standalone",
				"orientation": "portrait",
				"start_url": ".",
				"existing_manifest": "{\"name\": \"eBudget\", \"short_name\": \"eBudget\", \"display\": \"fullscreen\"}",
				"onConflict": "keep_existing",
				"declared": true
			},
			"assets": {
				"legacyIcon": false,
				"lowResolutionIcons": false
			}
		},
		"safariPinnedTab": {
			"pictureAspect": "black_and_white",
			"threshold": 60,
			"themeColor": "#796969"
		}
	},
	"settings": {
		"compression": 2,
		"scalingAlgorithm": "Mitchell",
		"errorOnImageTooSmall": true
	},
	"versioning": true
}